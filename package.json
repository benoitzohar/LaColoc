{
  "name": "la-coloc",
  "version": "3.0.0",
  "description": "NodeJS/Angular App to manage your flat, your couple or a week-end.",
  "main": "index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benoitzohar/LaColoc.git"
  },
  "keywords": [
    "es6",
    "nodejs",
    "coloc",
    "flatmates",
    "flat",
    "groceries",
    "expenses"
  ],
  "author": "Benoit Zohar <benoit.zohar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benoitzohar/LaColoc/issues"
  },
  "homepage": "https://github.com/benoitzohar/LaColoc#readme",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "start": "gulp serve",
    "debug": "cross-env DEBUG=lacoloc:* yarn start",
    "build": "gulp",
    "precommit": "yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register 'server/**/*.test.js' --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register 'server/**/*.test.js' --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "1.16.1",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.14.1",
    "express-validation": "^1.0.1",
    "express-winston": "2.2.0",
    "helmet": "3.4.1",
    "http-status": "^1.0.1",
    "joi": "^10.2.2",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.5",
    "mongoose": "4.8.5",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.4",
    "del": "^2.2.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "3.0.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.7",
    "husky": "^0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
